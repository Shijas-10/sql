INSERT INTO books (id, title, author, price, genre) 
VALUES (1, 'The Great Gatsby', 'F. Scott Fitzgerald', 450, 'Fiction'),
       (2, 'A Brief History of Time', 'Stephen Hawking', 600, 'Science'),
       (3,'The Alchemist', 'Paulo Coelho', 350, 'Fiction'),
       (4,'The Diary of a Young Girl', 'Anne Frank', 300, 'History'),
       (5 'The Da Vinci Code', 'Dan Brown', 500, 'Mystery');

       SELECT * FROM books WHERE price > 400;
       SELECT * FROM books WHERE genre IN ('History', 'Science', 'Fiction');
       SELECT * FROM books WHERE title = 'The Great Gatsby';
       SELECT * FROM books WHERE author != 'Dan Brown';




